@model List<ExchangeViewModel.Exchange_Rates>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<table class="table table-hover">
    <tr>
        <th>Döviz/th>
        <th>Kur</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td>
        </tr>
    }
</table>